{{#paper-card as |card|}}
  {{#card.title as |title|}}
    {{#title.text as |text|}}
      {{#text.headline class="h5 project"}}
        {{shared/inline-input value=project.name onChange=(action 'saveProject')}}
      {{/text.headline}}
      {{#text.subhead}}
        {{commands/ember-version path=project.path}}
      {{/text.subhead}}
    {{/title.text}}

    {{projects-list/project/menu project=project}}
  {{/card.title}}
{{/paper-card}}
